{
  "name": "san-antonio-tourist",
  "version": "0.0.0",
  "description": "",
  "files": [
    "dist"
  ],
  "main": "dist/main/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomaszkepa/san-antonio-tourist"
  },
  "scripts": {
    "clean": "rm -rf dist browser",
    "compile": "BABEL_ENV=production run-p compile:*",
    "postcompile": "BABEL_ENV=browser webpack --config webpack.config.dist.js",
    "compile:browser": "RUN_MODE=es babel src --out-dir browser --copy-files --source-maps --ignore \"src/test/**\"",
    "compile:node": "babel src --out-dir dist --copy-files --source-maps --ignore \"src/test/**\"",
    "flow": "flow",
    "flow:delete": "find ./src/main -name *.flow -delete",
    "flow:generate": "flow-copy-source -v ./src/main ./dist/main",
    "lint": "eslint src/**",
    "mocha": "BABEL_ENV=test mocha --require ./src/test/index.js 'src/test/unit/**/*.js'",
    "start": "BABEL_ENV=browser webpack-dev-server --config webpack.config.dev.js",
    "start:prod": "NODE_ENV=production node dist/main/index.js",
    "stylelint": "stylelint 'src/**/*.scss' --syntax scss",
    "pretest": "npm run flow:delete && run-p clean lint flow stylelint",
    "test": "BABEL_ENV=test nyc npm run mocha",
    "posttest": "npm run compile && npm run flow:generate"
  },
  "browser": "browser/main/index.js",
  "dependencies": {
    "babel-runtime": "^6.9.2",
    "classnames": "^2.2.5",
    "core-js": "^2.4.0",
    "ignore-styles": "^5.0.1",
    "immutable": "^3.8.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.0",
    "react-addons-pure-render-mixin": "^15.3.0",
    "react-dom": "^15.5.0",
    "react-redux": "^4.4.5",
    "recompose": "^0.25.0",
    "redux": "^3.5.2",
    "redux-immutable": "^3.0.0",
    "redux-saga": "^0.11.1",
    "reselect": "^2.5.3",
    "scriptjs": "^2.5.8"
  },
  "devDependencies": {
    "arrow-mocha": "^1.0.1",
    "autoprefixer": "^6.6.1",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-rewire": "^1.0.0-rc-6",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.22.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-statics": "0.0.5",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "babel-template": "^6.14.0",
    "babel-traverse": "^6.14.0",
    "babel-types": "^6.14.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.2",
    "chai-immutable": "^1.6.0",
    "clean-webpack-plugin": "^0.1.10",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-flowtype": "^2.4.0",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.37.0",
    "flow-copy-source": "^1.1.0",
    "jsdom": "^9.11.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.5.3",
    "mocha-sinon": "^1.1.6",
    "nightwatch": "^0.9.12",
    "node-sass": "^3.8.0",
    "npm-run-all": "^2.3.0",
    "nyc": "^7.1.0",
    "postcss-loader": "^1.2.1",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.5.0",
    "redux-devtools-extension": "^1.0.0",
    "sass-loader": "^4.0.0",
    "sass-mq": "^3.2.9",
    "selenium-download": "^2.0.9",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "stylelint": "^7.8.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-order": "^0.2.2",
    "svg-sprite-loader": "^2.0.4",
    "svgo-loader": "^1.2.1",
    "wdio-mocha-framework": "^0.5.10",
    "wdio-selenium-standalone-service": "0.0.8",
    "wdio-spec-reporter": "^0.1.0",
    "webdriverio": "^4.8.0",
    "webfonts-loader": "^1.2.0",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0",
    "webpack-hot-middleware": "^2.12.2"
  },
  "engines": {
    "node": ">=6.9"
  },
  "nyc": {
    "lines": 0,
    "statements": 50,
    "functions": 50,
    "branches": 50,
    "include": [
      "src/main/**/*.js"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "require": [
      "babel-register"
    ],
    "extension": [
      ".jsx",
      ".js"
    ],
    "sourceMap": true,
    "cache": false,
    "check-coverage": true,
    "report-dir": "./coverage"
  }
}
